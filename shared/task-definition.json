{
    "containerDefinitions": [
      {
        "essential": true,
        "name": "kiosk",
        "image": "471808368523.dkr.ecr.us-east-1.amazonaws.com/kiosk:latest",
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ]
      }
    ],
    "cpu": "256",
    "family": "fargate-task-definition",
    "memory": "512",
    "networkMode": "awsvpc",
    "runtimePlatform": {
      "operatingSystemFamily": "LINUX"
    },
    "requiresCompatibilities": ["FARGATE"]
  }